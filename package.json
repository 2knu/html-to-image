{
  "name": "html-to-image",
  "version": "0.0.1",
  "description": "Generates an image from a DOM node using HTML5 canvas.",
  "main": "./lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha ./test/index.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --ext .js,.jsx src",
    "build": "rimraf lib && babel src --out-dir lib",
    "prebuild": "npm run lint",
    "prepublish": "npm run build",
    "precommit": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bubkoo/html-to-image.git"
  },
  "keywords": [
    "screenshot",
    "cavas",
    "html",
    "dom",
    "render",
    "image",
    "vector",
    "svg"
  ],
  "author": "bubkoo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bubkoo/html-to-image/issues"
  },
  "homepage": "https://github.com/bubkoo/html-to-image#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.2"
  }
}
